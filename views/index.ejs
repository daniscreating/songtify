		<div class="box-nav d-flex-justify-between">
			<a href="/add-song" class="border-shadow"><span class="text-gradient">New Song<i class="fa-solid fa-music"></i></span></a>
			
		</div>

		<!-- form handling -->
		<form action="/" method="POST">
			<table class="table">
				<thead class="thead-dark">
					<tr>
						<th>Song</th>
						<th>Artist</th>
						<th>Album</th>
